---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';
---

<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={`Success Stories - ${SITE_TITLE}`} description="Real client transformations and success stories from PR Wave's social media marketing services and SMM PLUS course." />
	</head>
	<body>
		<Header />
		
		<!-- Hero Section -->
		<section class="hero">
			<div class="container">
				<div class="hero-content">
					<h1 class="hero-title" data-translate="hero.title">Success Stories</h1>
					<p class="hero-description" data-translate="hero.description">
						Discover how businesses transformed their digital presence with PR Wave. 

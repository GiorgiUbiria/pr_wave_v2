</html>

<style>
	/* Hero Section */
	.hero {
		background: var(--wave-gradient);
		color: var(--white);
		text-align: center;
		padding: var(--spacing-xl) 0;
	}

	.hero-title {
		font-size: 3rem;
		font-weight: 800;
		margin-bottom: var(--spacing-md);
		color: var(--white);
	}

	.hero-description {
		font-size: 1.2rem;
		opacity: 0.9;
		max-width: 600px;
		margin: 0 auto;
	}

	/* Featured Story */
	.featured-story {
		padding: var(--spacing-xl) 0;
	}

	.featured-card {
		background: var(--white);
		border-radius: var(--border-radius-lg);
		box-shadow: 0 8px 30px rgba(0, 0, 0, 0.1);
		overflow: hidden;
		display: grid;
		grid-template-columns: 1fr 1fr;
		gap: 0;
		min-height: 500px;
	}

	.featured-content {
		padding: var(--spacing-xl);
		display: flex;
		flex-direction: column;
		justify-content: center;
	}

	.featured-badge {
		background: var(--success-color);
		color: var(--white);
		padding: var(--spacing-xs) var(--spacing-sm);
		border-radius: var(--border-radius);
		font-size: 0.8rem;
		font-weight: 600;
		width: fit-content;
		margin-bottom: var(--spacing-md);
	}

	.featured-content h2 {
		font-size: 2.2rem;
		font-weight: 700;
		margin-bottom: var(--spacing-md);
		color: var(--text-heading);
	}

	.featured-content p {
		font-size: 1.1rem;
		color: var(--text-secondary);
		margin-bottom: var(--spacing-lg);
		line-height: 1.6;
	}

	.metrics-grid {
		display: grid;
		grid-template-columns: 1fr 1fr;
		gap: var(--spacing-md);
	}

	.metric {
		text-align: center;
		padding: var(--spacing-md);
		background: #f8f9fa;
		border-radius: var(--border-radius);
	}

	.metric-number {
		font-size: 2rem;
		font-weight: 800;
		color: var(--ocean-blue);
		margin-bottom: var(--spacing-xs);
	}

	.metric-label {
		font-size: 0.9rem;
		color: var(--text-secondary);
	}

	.featured-visual {
		background: linear-gradient(135deg, #e3f2fd 0%, #bbdefb 100%);
		padding: var(--spacing-xl);
		display: flex;
		align-items: center;
		justify-content: center;
	}

	.before-after {
		display: flex;
		align-items: center;
		gap: var(--spacing-lg);
		text-align: center;
	}

	.comparison-item h4 {
		margin-bottom: var(--spacing-md);
		color: var(--text-heading);
	}

	.comparison-arrow {
		font-size: 2rem;
		color: var(--ocean-blue);
		font-weight: bold;
	}

	.stats-before p,
	.stats-after p {
		margin: var(--spacing-xs) 0;
		font-size: 0.9rem;
	}

	/* Case Studies */
	.cases-grid {
		gap: var(--spacing-lg);
	}

	.case-card {
		background: var(--white);
		border-radius: var(--border-radius-lg);
		padding: var(--spacing-xl);
		box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
		transition: transform var(--transition-normal);
	}

	.case-card:hover {
		transform: translateY(-5px);
	}

	.case-header {
		display: flex;
		align-items: center;
		gap: var(--spacing-md);
		margin-bottom: var(--spacing-lg);
	}

	.case-icon {
		font-size: 3rem;
		width: 80px;
		height: 80px;
		display: flex;
		align-items: center;
		justify-content: center;
		background: var(--wave-gradient);
		border-radius: 50%;
		flex-shrink: 0;
	}

	.case-info h3 {
		color: var(--text-heading);
		margin-bottom: var(--spacing-xs);
	}

	.case-industry {
		color: var(--text-secondary);
		font-size: 0.9rem;
		margin: 0;
	}

	.case-results {
		display: flex;
		gap: var(--spacing-lg);
		margin-bottom: var(--spacing-lg);
	}

	.result-item {
		text-align: center;
	}

	.result-number {
		display: block;
		font-size: 1.8rem;
		font-weight: 800;
		color: var(--ocean-blue);
		margin-bottom: var(--spacing-xs);
	}

	.result-label {
		font-size: 0.8rem;
		color: var(--text-secondary);
	}

	.case-description {
		color: var(--text-primary);
		line-height: 1.6;
		margin-bottom: var(--spacing-lg);
	}

	.case-services {
		display: flex;
		flex-wrap: wrap;
		gap: var(--spacing-xs);
	}

	.service-tag {
		background: #e3f2fd;
		color: var(--ocean-blue);
		padding: var(--spacing-xs) var(--spacing-sm);
		border-radius: var(--border-radius);
		font-size: 0.8rem;
		font-weight: 500;
	}

	/* Graduate Success */
	.graduate-success {
		background: var(--wave-gradient);
		color: var(--white);
	}

	.graduate-success .section-title,
	.graduate-success .section-subtitle {
		color: var(--white);
	}

	.graduates-grid {
		gap: var(--spacing-lg);
	}

	.graduate-card {
		background: rgba(255, 255, 255, 0.1);
		backdrop-filter: blur(10px);
		border-radius: var(--border-radius-lg);
		padding: var(--spacing-xl);
		text-align: center;
		transition: transform var(--transition-normal);
	}

	.graduate-card:hover {
		transform: translateY(-5px);
	}

	.graduate-avatar {
		font-size: 4rem;
		margin-bottom: var(--spacing-md);
	}

	.graduate-card h4 {
		color: var(--white);
		margin-bottom: var(--spacing-xs);
	}

	.graduate-role {
		color: rgba(255, 255, 255, 0.8);
		font-size: 0.9rem;
		margin-bottom: var(--spacing-md);
	}

	.graduate-story {
		color: rgba(255, 255, 255, 0.9);
		line-height: 1.6;
		margin-bottom: var(--spacing-lg);
		font-style: italic;
	}

	.achievement-badge {
		background: var(--success-color);
		color: var(--white);
		padding: var(--spacing-xs) var(--spacing-md);
		border-radius: var(--border-radius);
		font-size: 0.8rem;
		font-weight: 600;
	}

	/* Testimonials */
	.testimonials-slider {
		display: grid;
		grid-template-columns: repeat(2, 1fr);
		gap: var(--spacing-lg);
	}

	.testimonial-item {
		background: var(--white);
		border-radius: var(--border-radius-lg);
		padding: var(--spacing-xl);
		box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);
	}

	.testimonial-stars {
		font-size: 1.2rem;
		margin-bottom: var(--spacing-md);
	}

	.testimonial-text {
		font-style: italic;
		line-height: 1.6;
		margin-bottom: var(--spacing-lg);
		color: var(--text-primary);
	}

	.testimonial-author h4 {
		margin-bottom: var(--spacing-xs);
		color: var(--text-heading);
	}

	.testimonial-author p {
		color: var(--text-secondary);
		font-size: 0.9rem;
		margin: 0;
	}

	/* CTA Section */
	.cta-section {
		background: var(--wave-gradient);
		color: var(--white);
		text-align: center;
	}

	.cta-section .section-title,
	.cta-section .section-subtitle {
		color: var(--white);
	}

	.cta-buttons {
		display: flex;
		gap: var(--spacing-md);
		justify-content: center;
		flex-wrap: wrap;
		margin-top: var(--spacing-lg);
	}

	/* Responsive Design */
	@media (max-width: 768px) {
		.hero-title {
			font-size: 2.5rem;
		}

		.featured-card {
			grid-template-columns: 1fr;
		}

		.featured-visual {
			padding: var(--spacing-lg);
		}

		.before-after {
			flex-direction: column;
			gap: var(--spacing-md);
		}

		.comparison-arrow {
			transform: rotate(90deg);
		}

		.cases-grid {
			grid-template-columns: 1fr;
		}

		.graduates-grid {
			grid-template-columns: 1fr;
		}

		.testimonials-slider {
			grid-template-columns: 1fr;
		}

		.metrics-grid {
			grid-template-columns: 1fr;
		}

		.case-results {
			justify-content: space-around;
		}

		.cta-buttons {
			flex-direction: column;
			align-items: center;
		}
	}
</style>

<script>
	// Translation functionality
	let currentLanguage = localStorage.getItem('language') || 'en';

	const translations = {
		en: {
			'hero.title': 'Success Stories',
